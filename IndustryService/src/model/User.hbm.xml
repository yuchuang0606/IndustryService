<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
<class name="User" table="tpf_user">
<id name="userid">
<generator class="native"/>
</id>
<property name="username"/>
<property name="email"/>
<property name="password"/>
<property name="usergroup"/>
<property name="phone"/>
<property name="qq"/>
<property name="address"/>
<property name="userpic"/>
<property name="regdate"/>
<property name="lastlogin" type="timestamp" insert="false" update="false"/>
<property name="logintimes"/>
<property name="uploadfilenumber"/>
<property name="downloadfilenumber"/>
<property name="credit"/>
<property name="coin"/>
<property name="gender"/>
<property name="birthdate"/>
<property name="company"/>
<property name="industry"/>
<property name="department"/>
<property name="jobtitle"/>
<property name="postcode"/>
<property name="mailaddress"/>
<property name="introduction"/>
<property name="uploadsize"/>
</class>
</hibernate-mapping>